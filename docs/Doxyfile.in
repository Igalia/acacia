DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = Acacia
OUTPUT_LANGUAGE        = English

OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/docs/

INPUT                 += @CMAKE_CURRENT_SOURCE_DIR@/../include/acacia/atspi
INPUT                 += @CMAKE_CURRENT_SOURCE_DIR@/../include/acacia/mac
INPUT                 += @CMAKE_CURRENT_SOURCE_DIR@/../include/acacia/ia2

EXCLUDE_PATTERNS       = @DOXY_EXCLUDE@

GENERATE_TREEVIEW      = YES

INPUT                 += @CMAKE_SOURCE_DIR@/README.md
USE_MDFILE_AS_MAINPAGE = @CMAKE_SOURCE_DIR@/README.md

// To remove the top bar menu
DISABLE_INDEX = YES
GENERATE_TREEVIEW = YES
FULL_SIDEBAR = NO

// To run on search
SEARCHENGINE = YES
SERVER_BASED_SEARCH = NO.
